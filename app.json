{
  "expo": {
    "orientation": "default",
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "ios": {
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app needs access to your health data to display your fitness statistics.",
        "NSHealthUpdateUsageDescription": "This app needs permission to save workout data to your Health app.",
        "UIBackgroundModes": ["processing"]
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": []
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "experiments": {
      "reactCompiler": true
    },
    "plugins": [
      "expo-font",
      "expo-image",
      "expo-router",
      "expo-web-browser"
    ]
  }
}
